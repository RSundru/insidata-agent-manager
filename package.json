{
  "name": "vapi-server-sdk",
  "version": "1.0.0",
  "description": "Enhanced VAPI Server SDK with monitoring, webhooks, and recording management",
  "main": "vapi-agent.js",
  "type": "module",
  "scripts": {
    "start": "node vapi-agent.js",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "vapi",
    "voice",
    "ai",
    "assistant",
    "call",
    "monitoring",
    "webhooks"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vapi-ai/server-sdk": "^0.10.0",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/register": "^7.28.3",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "mocha": "^10.8.2",
    "nock": "^13.5.6",
    "prettier": "^3.1.1",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.js": [
